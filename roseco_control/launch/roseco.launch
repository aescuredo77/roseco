<launch>
    <arg name="rvizconfig" default="$(find roseco_description)/rviz/roseco_description.rviz" />
    <arg name="world_name" default="$(find roseco_gazebo)/worlds/corridor.world"/>

    <include file="$(find roseco_gazebo)/launch/roseco_view.launch">
        <!--arg name="model" value="$(arg model)" /-->
        <arg name="world_name" value="$(arg world_name)"/>
    </include>

    <include file="$(find roseco_control)/launch/roseco_control.launch">
        <!-- arg name="model" value="$(arg model)" /-->
    </include>

    <!-- Launch visualization in rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
</launch>